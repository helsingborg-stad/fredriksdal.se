            "url":"https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "humanmade/page-for-post-type",
                "version": "1.0.1",
                "dist": {
                    "url": "https://github.com/humanmade/page-for-post-type/archive/4104277b2140a5b5badb6796979c6e2373c25048.zip",
                    "type": "zip"
                },
                "type": "wordpress-plugin"
            }
        }
